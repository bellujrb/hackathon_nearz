{
  "compilerOptions": {
    "lib": ["es6", "dom", "es2015", "ES2020.BigInt"],
    "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./dist",
      "rootDir": "./",
      "esModuleInterop": true,
      "sourceMap": true,
      "noEmit": true,
      "noImplicitAny": false,
      "noEmitOnError": true,
      "removeComments": false,
      "experimentalDecorators": true,
      "emitDecoratorMetadata" : true,
      "strict": false,
 }
}
